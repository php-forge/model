{
    "name": "forge/model",
    "type": "library",
    "description": "Simple model for Yii3.",
    "keywords": [
        "yii",
        "simple",
        "model"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "yiisoft/arrays": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "roave/infection-static-analysis-plugin": "*",
        "vimeo/psalm": "*"
    },
    "autoload": {
        "psr-4": {
            "Forge\\Model\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forge\\Model\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
